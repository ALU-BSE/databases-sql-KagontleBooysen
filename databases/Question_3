    { buyer_id: ObjectId("buyer_id_here"), product_id: ObjectId("product_id_here"), date: ISODate("2024-03-24"), quantity: 1 },
    { buyer_id: ObjectId("buyer_id_here"), product_id: ObjectId("product_id_here"), date: ISODate("2024-03-24"), quantity: 2 }
